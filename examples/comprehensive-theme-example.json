{
  "description": "Comprehensive theme example showing all available GraphQL attributes",
  "example": {
    "themeTokens": {
      "ref": {
        "palette": {
          "primary50": "#353F47",
          "supportOne50": "#007681",
          "supportTwo50": "#2A565B",
          "supportThree50": "#466B4D",
          "supportFour50": "#7F6A4D",
          "supportFive50": "#914125",
          "supportSix50": "#D5B65D"
        }
      },
      "sys": {
        "color": {
          "background": "#F6F7F8"
        },
        "borderRadius": {
          "xl": "4px"
        },
        "elevation": {
          "surface": "2px 2px 10px 2px rgba(0, 0, 0, 0.10), 0px 1px 2px 0px rgba(0, 0, 0, 0.10)"
        }
      },
      "comp": {
        "lineChart": {
          "tension": 0,
          "color": "#00384D",
          "showAxis": true
        },
        "button": {
          "borderRadius": "4px",
          "lg": {
            "height": "40px",
            "fontSize": "16px"
          },
          "filled": {
            "primary": {
              "enabled": {
                "container": {
                  "color": "#CAD5D6"
                },
                "text": {
                  "color": "#0dd4c6"
                }
              }
            }
          },
          "outlined": {
            "primary": {
              "enabled": {
                "container": {
                  "color": "#1c288a"
                },
                "text": {
                  "color": "#1c288a"
                }
              }
            }
          }
        },
        "tabNav": {
          "background": {
            "active": "#8a1c4c"
          }
        },
        "selectionChip": {
          "borderRadius": "4px",
          "background": {
            "active": "#CAD5D6"
          }
        },
        "textField": {
          "textColor": "#1421de",
          "labelColor": "#1421de",
          "fontSize": "16px",
          "borderColor": "#1421de",
          "errorColor": "#8a811c"
        },
        "helpText": {
          "errorColor": "#8a811c"
        },
        "statusBar": {
          "containerColor": "#8a811c",
          "borderColor": "#1421de",
          "borderRadius": 10,
          "activeColor": "#CAD5D6"
        },
        "typography": {
          "headingSmall": {
            "fontFamily": "Tiempos Headline",
            "fontSize": "24px"
          },
          "bodySmall": {
            "fontSize": "20px",
            "fontWeight": 600,
            "lineHeight": "16px",
            "letterSpacing": "0.25px"
          }
        }
      }
    }
  },
  "usage": {
    "description": "Use the update_organization_theme tool with any combination of these properties",
    "note": "All properties are optional - you can update just the specific theme elements you need",
    "examples": [
      "Update only colors: { \"themeTokens\": { \"ref\": { \"palette\": { \"primary50\": \"#ff0000\" } } } }",
      "Update button styling: { \"themeTokens\": { \"comp\": { \"button\": { \"borderRadius\": \"8px\" } } } }",
      "Update typography: { \"themeTokens\": { \"comp\": { \"typography\": { \"headingSmall\": { \"fontSize\": \"28px\" } } } } }"
    ]
  }
}
